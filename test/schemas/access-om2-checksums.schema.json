{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Access-OM2 Checksums",
    "description": "The checksums from the model output of Access-OM2 model run",
    "type": "object",
    "properties": {
        "schema": {
            "type": "string"
        },
        "version": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "output": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "field": {
                        "type": "string"
                    },
                    "checksum": {
                        "type": "string"
                    }
                },
                "required": [
                    "field",
                    "checksum"
                ]
            }
        }
    },
    "required": [
        "schema",
        "version",
        "output"
    ],
    "additionalProperties": false
}